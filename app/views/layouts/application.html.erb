<!DOCTYPE html>
<html>
  <head>
    <title>ComHelp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <script src="https://kit.fontawesome.com/18f2e2cbfa.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag 'application', media: 'all'%>
    
  </head>

  <body>

      <%= render 'layouts/navbar'%>
      <%= render 'layouts/flash'%>

      <% flash.each do |key, value| %>
      <p><%= value %></p>
      <% end %>

    <%= yield %>
    <%= javascript_include_tag 'application' %>

     <%= render 'layouts/footer'%>

  </body>

  
</html>
